{
  "name": "history-server",
  "version": "1.0.0",
  "description": "Exevo Pan history auctions server",
  "author": "xandjiji <xandjiji@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "sucrase-node src",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports && cp ./src/Data/*.json ./dist/Data && cp ./src/Data/*.jsonl ./dist/Data",
    "start": "node ./dist"
  },
  "dependencies": {
    "auction-queries": "*",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "logging": "*",
    "shared-utils": "*"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/exevo-pan": "*",
    "@types/express": "^4.17.13",
    "config": "*",
    "sucrase": "^3.20.3",
    "typescript": "^4.3.2"
  }
}
